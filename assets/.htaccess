<IfModule mod_authz_core.c>
    Require all denied
</IfModule>

<FilesMatch "(?i)\.(php5|php4|php|php3|php2|phtml|pl|py|jsp|asp|htm|shtml|html|sh|cgi|cache)$">
    <IfModule mod_authz_core.c>
        Require all denied
    </IfModule>
</FilesMatch>

<FilesMatch "(?i)\.(jpg|jpeg|png|gif|svg|flv|mp3|jfif|wav|mp4|ogg|pdf|webm|bmp|webp|json|woff|woff2|ttf|eot|js|css|map|mov)$">
    <IfModule mod_authz_core.c>
        Require all granted
    </IfModule>
</FilesMatch>